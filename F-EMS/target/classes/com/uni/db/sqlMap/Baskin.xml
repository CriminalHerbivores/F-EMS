<sqlMap namespace="Baskin">
	<typeAlias alias="str" type="java.lang.String" />
	<typeAlias alias="int" type="java.lang.Integer" />
	<typeAlias alias="baskin" type="com.uni.fems.dto.BaskinVO" />
	
	<select id="getThemeBaskin" parameterClass="str" resultClass="baskin">
		select * from Baskin where bskn_Nm = #bskn_Nm#
	</select>
	<select id="getBaskin" parameterClass="str" resultClass="baskin">
		select bskn_Nm from Baskin
	</select>
	
</sqlMap>
