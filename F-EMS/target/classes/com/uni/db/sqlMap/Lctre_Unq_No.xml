	<typeAlias alias="str" type="java.lang.String" />
	<typeAlias alias="int" type="java.lang.Integer" />
	<typeAlias alias="lctre_unq_no" type="com.uni.fems.dto.Lctre_Unq_NoVO" />
<typeAlias alias="lctre_Search" type="com.uni.fems.dto.Lctre_SearchVO" />
	 
	 
	 <!-- 교수가 강의명으로 강의 검색 -->
	 <select id="selectLctreByName" parameterClass="str" resultClass="lctre_Search">
		select * from subjct_view
		where Lu_Lctre_Nm like '%'||#Lu_Lctre_Nm#||'%'
		or lu_lctre_code like '%'||#lu_lctre_code#||'%'
	</select>


