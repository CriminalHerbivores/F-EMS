<sqlMap namespace="Test">
	<typeAlias alias="str" type="java.lang.String" />	
	<typeAlias alias="int" type="java.lang.Integer" />
	<typeAlias alias="test" type="com.uni.fems.dto.TestVO" />
	
	<select id="listAllTest" parameterClass="int" resultClass="test">
		select * from test where te_tp_no=#te_Tp_No# order by te_ques_no
	</select>
	
	<update id="insertTest" parameterClass="test">
		insert into test(te_ques_no,te_tp_no,te_ques,te_ca,te_no1,te_no2,te_no3,te_no4) 
		values(te_seq.nextval,#te_Tp_No#,#te_Ques#,#te_Ca#,#te_No1#,#te_No2#,#te_No3#,#te_No4#)
	</update>
	
	<update id="updateTest" parameterClass="test">
		update test set te_ques=#te_Ques#, te_ca=#te_Ca#, 
		te_no1=#te_No1#,te_no2=#te_No2#,te_no3=#te_No3#,te_no4=#te_No4#
		 where te_ques_no=#te_Ques_No#
	</update>
	
	<update id="deleteTest" parameterClass="int">
		delete from test where te_ques_no=#te_Ques_No#
	</update>
	
	<update id="deleteTestForTestPaper" parameterClass="int">
		delete from test where te_tp_no=#te_Tp_No#
	</update>
	
</sqlMap>