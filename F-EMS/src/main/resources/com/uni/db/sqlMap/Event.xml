<sqlMap namespace="Event">
	<typeAlias alias="str" type="java.lang.String" />
	<typeAlias alias="int" type="java.lang.Integer" />
	<typeAlias alias="event" type="com.uni.fems.dto.EventVO" />
	
	<select id="selectEvent" resultClass="event">
		select * from event
	</select>
	
	<update id="updateEvent">
		update event
		set 
			eve_Start=#eve_Start#,
			eve_End=#eve_End#,
			eve_Useyn=#eve_Useyn#
		where 
			eve_Nm = #eve_Nm#
	</update>
	
</sqlMap>